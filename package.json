    "prepare": "husky install",
    "generate:3d-compression": "sh scripts/3d-compression.sh",
    "editor": "magic",
    "editor:build": "magic build --base \"editor\" --output \"out/editor\""
  },
  "dependencies": {
    "@magic-circle/client": "0.2.2",
    "@next/bundle-analyzer": "^12.3.1",
