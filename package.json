    "@svgr/webpack": "^6.3.1",
    "classnames": "^2.3.2",
    "csv-parse": "^5.3.4",
    "date-fns": "^2.29.3",
    "detect-gpu": "^5.0.16",
    "next": "^13.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.11",
    "react-use": "^17.4.0",
    "sass": "^1.57.1",
    "sharp": "^0.32.0",
    "suncalc": "^1.9.0",
    "swiper": "^9.1.0",
    "three": "^0.148.0"
  },
  "devDependencies": {
    "@gltf-transform/cli": "^2.5.1",
    "@gltf-transform/core": "^2.5.1",
