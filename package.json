    "@types/node": "18.7.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.24",
    "@types/three": "^0.148.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "express-force-https": "^1.0.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "image-size": "^1.0.2",
    "lint-staged": "^13.0.2",
    "node-fetch": "^3.3.0",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "slugify": "^1.6.5",
    "typescript": "4.7.4",
    "url-loader": "^4.1.1"
